{
	"name": "@upradata/rxjs",
	"author": {
		"name": "Thomas Milotti (Upra-Data)",
		"email": "thomas.milotti@gmail.com",
		"url": "https://github.com/upradata"
	},
	"repository": {
		"url": "https://github.com/upradata/rxjs.git"
	},
	"license": "MIT",
	"version": "1.0.1",
	"description": "Set of rxjs utilities",
	"sideEffects": false,
	"main": "lib",
	"types": "lib-esm",
	"module": "lib-esm",
	"files": [
		"lib",
		"lib-esm"
	],
	"scripts": {
		"clean": "shx rm -rf lib lib-esm",
		"pre-build": "pnpm run clean",
		"build": "concurrently 'tsc -p tsconfig.lib.json' 'tsc -p tsconfig.lib-esm.json'",
		"post-build": "pnpm run test && npm run e2e",
		"watch": "pnpm run build:watch",
		"build:watch": "concurrently 'tsc -p tsconfig.lib.json -w' 'tsc -p tsconfig.lib-esm.json -w'",
		"test": "pnpm jest --passWithNoTests --verbose false --config jest-ut.config.js",
		"e2e": "pnpm jest --passWithNoTests --verbose false --config jest-e2e.config.js",
		"github-push": "pnpm run build && npm version patch && git pushall && git pushall-tags",
		"npm-publish": "pnpm run build && npm publish --access public"
	},
	"devDependencies": {
		"@types/fs-extra": "^9.0.12",
		"@types/jest": "^26.0.20",
		"@types/node": "^14.14.21",
		"concurrently": "^5.3.0",
		"fs-extra": "^10.0.0",
		"jest": "^26.6.3",
		"shx": "^0.3.3",
		"ts-jest": "^26.4.4",
		"ts-node": "^9.1.1",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^4.1.3"
	},
	"keywords": [
		"typescript",
		"util"
	],
	"publishConfig": {
		"access": "public"
	},
	"local": {
		"dependencies": {
			"@upradata/util": "link:/home/milottit/Libraries/Upra-Data/util@11.2.9"
		}
	},
	"dependencies": {
		"rxjs": "^7.2.0"
	}
}
